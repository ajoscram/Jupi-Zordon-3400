{
  "name": "jupi-zordon-3400",
  "version": "0.0.1",
  "description": "A bot to record League of Legends player statistics",
  "main": "build/main.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "exec": "node build/src/main.js",
    "build": "npx tsc",
    "jasmine": "npx jasmine-ts \"test/**/*.spec.ts\"",
    "start": "npm run build && npm run exec",
    "dev": "npx nodemon -e ts,json,env --ignore ./build --ignore *.spec.ts --exec npm run start --s",
    "test": "npx nodemon -e ts,json,env --ignore ./build --exec \"npm run jasmine\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajoscram/Jupi-Zordon-3400.git"
  },
  "keywords": [
    "bot",
    "LoL",
    "fun"
  ],
  "author": "Coopemalza S.A.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ajoscram/Jupi-Zordon-3400/issues"
  },
  "homepage": "https://github.com/ajoscram/Jupi-Zordon-3400#readme",
  "devDependencies": {
    "@types/jasmine": "^3.7.7",
    "dotenv": "^10.0.0",
    "jasmine": "^3.8.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.9",
    "typescript": "^4.3.5"
  }
}
